  ---
  import Head from '../commons/commons.head.astro'

  let { course } = Astro.props
  let { lesson } = Astro.props

  let title   = lesson.key ('title')
  let caption = course.key ('title')
  ---

  <div class="ui container">
    <div class="talk">
      <div class="head"><Head title={title} caption={caption}/></div>
    </div>
  </div>

  <style>
    .talk {
      margin-top: 100px;
    }
  </style>