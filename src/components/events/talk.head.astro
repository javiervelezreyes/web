---
import Head from '../commons/commons.head.astro'

let { talk } = Astro.props

let title    = talk.key ('title')
let caption  = talk.key ('caption')
---

<div class="ui container">
  <div class="talk">
    <div class="head"><Head title={title} caption={caption}/></div>
  </div>
</div>

<style>
  .talk {
    margin-top: 100px;
  }
</style>