---
import Head from '../commons/commons.head.astro'

let { event } = Astro.props

let title    = event.key ('title')
let caption  = event.key ('caption')
---

<div class="ui container">
  <div class="event">
    <div class="head"><Head title={title} caption={caption}/></div>
  </div>
</div>

<style>
  .event {
    margin-top: 100px;
  }
</style>