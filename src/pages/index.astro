---
import Config   from '../config/config.js'
import Layout   from '../layouts/layout.home.astro'
import Hero     from '../components/home/hero.astro'
import About    from '../components/home/about.astro'
import Activity from '../components/home/activity.astro'
import Contact  from '../components/home/contact.astro'
import Quotes   from '../components/home/quotes.astro'

let brand = Config.key ('site.brand')
---

<Layout title={brand}>
  <Hero/>
  <About/>
  <Activity/>
  <Quotes/>
  <Contact/>
</Layout>

